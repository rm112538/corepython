    "# Hanoi Tower Solver\n",
    "def solve_tower(num_disks, start, end, extra):\n",
    "    if (num_disks > 0):\n",
    "        solve_tower(num_disks-1, start, extra, end)\n",
    "        print('Move top disk from', start, 'to', end)\n",
    "        solve_tower(num_disks-1, extra, end, start)\n",
    "solve_tower(3, 'a', 'c','b')"
   ]
